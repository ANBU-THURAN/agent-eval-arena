# API Keys
GOOGLE_API_KEY=your_google_api_key_here

# Demo API Key (for manual session start)
DEMO_API_KEY=your_demo_api_key_here

# Database (use either local file or remote Turso)
# For local SQLite file:
# DATABASE_URL=file:./data/arena.db
# For remote Turso:
DATABASE_URL=libsql://your-database.turso.io
DATABASE_AUTH_TOKEN=your_turso_auth_token_here

# Server
PORT=3000
NODE_ENV=development

# Trading Configuration
SESSION_START_TIME=14:00
TIMEZONE=UTC
